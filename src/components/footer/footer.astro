---
import { routingitems } from "../../data/routing.astro"
---
<nav class="bg-gray-800 p-4">
  <div class="container mx-auto flex justify-between items-center">
    <ul class="flex space-y-4">
      {routingitems.map((item) => (
        <li>
          <a href={item.href} class="text-white hover:text-gray-300">{item.name}</a>
        </li>
      ))}
    </ul>
  </div>
</nav>